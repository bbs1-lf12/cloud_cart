{% set app_options = getAppOptions() %}
<!DOCTYPE html>
<html class="no-js" lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <title>CloudCart</title>
    <meta name="description" content="" />

    {% block stylesheets %}
        {{ vite_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ vite_entry_script_tags('app') }}
    {% endblock %}
</head>
<body>
{% embed 'common/navbar/_navbar.html.twig' %}
    {% block navbarMenu %}
        {% include 'admin/partials/_navbar_links.html.twig' %}
    {% endblock %}
{% endembed %}
<div class="container mx-auto">
    {% block body %}{% endblock %}
</div>
{% include 'common/popup/_confirm_popup.html.twig' %}
</body>
</html>
